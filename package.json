{
  "name": "beautify-server",
  "version": "1.0.0",
  "private": true,
  "description": "API version 1.0 for beautify",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "dev": "concurrently \"npm:redis:start\" \"npm:server:dev\"",
    "test": "concurrently \"npm:redis:start\" \"npm:server:test\" \"npm:cypress:open\"",
    "server:dev": "nodemon -r dotenv/config server.js dotenv_config_path=./config/envs/.env.dev",
    "server:test": "nodemon -r dotenv/config server.js dotenv_config_path=./config/envs/.env.test",
    "jest:test": "jest --runInBand --bail --testTimeout 10000",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "redis:start": "redis-server"
  },
  "author": "nukuutos",
  "license": "ISC",
  "engines": {
    "node": "14.17.6"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dayjs": "^1.10.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.1.0",
    "express-validator": "^6.3.1",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "mongodb": "^4.1.3",
    "multer": "^1.4.2",
    "redis": "^3.1.2",
    "sharp": "^0.26.3",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.0",
    "concurrently": "^6.4.0",
    "cypress": "^9.1.1",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-node": "^11.1.0",
    "image-size": "^1.0.0",
    "jest": "^27.2.5",
    "jest-json-schema": "^6.0.0",
    "nodemon": "^2.0.2",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6"
  },
  "jest": {
    "testEnvironment": "node",
    "globalSetup": "<rootDir>/tests/global.js",
    "testPathIgnorePatterns": [
      "<rootDir>/cypress"
    ]
  }
}
